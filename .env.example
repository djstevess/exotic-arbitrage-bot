# Exotic Arbitrage Bot Environment Variables
# Copy this file to .env.local and fill in your values

# App Configuration
NEXT_PUBLIC_APP_NAME="Exotic Arbitrage Bot"
NEXT_PUBLIC_MIN_PROFIT_THRESHOLD=0.08
NEXT_PUBLIC_MIN_LIQUIDITY=1500
NEXT_PUBLIC_UPDATE_INTERVAL=8000

# API Keys for Real DEX Integration (Optional)
# Solana/Raydium
RAYDIUM_API_KEY=your_raydium_api_key_here
SERUM_API_KEY=your_serum_api_key_here

# Avalanche/Trader Joe
TRADERJOE_API_KEY=your_traderjoe_api_key_here
PANGOLIN_API_KEY=your_pangolin_api_key_here

# Cosmos/Osmosis
OSMOSIS_API_KEY=your_osmosis_api_key_here
JUNO_API_KEY=your_juno_api_key_here

# Arbitrum
GMX_API_KEY=your_gmx_api_key_here
CAMELOT_API_KEY=your_camelot_api_key_here

# Base
AERODROME_API_KEY=your_aerodrome_api_key_here

# Optimism
VELODROME_API_KEY=your_velodrome_api_key_here

# Fantom
SPOOKYSWAP_API_KEY=your_spookyswap_api_key_here
SPIRITSWAP_API_KEY=your_spiritswap_api_key_here

# BSC
PANCAKESWAP_API_KEY=your_pancakeswap_api_key_here

# Blast
THRUSTER_API_KEY=your_thruster_api_key_here

# Price Feed APIs
COINGECKO_API_KEY=your_coingecko_api_key_here
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key_here
DEFILLAMA_API_KEY=your_defillama_api_key_here

# RPC Endpoints for Direct Blockchain Access
ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc
OPTIMISM_RPC_URL=https://mainnet.optimism.io
BASE_RPC_URL=https://mainnet.base.org
AVALANCHE_RPC_URL=https://api.avax.network/ext/bc/C/rpc
FANTOM_RPC_URL=https://rpc.ftm.tools
BSC_RPC_URL=https://bsc-dataseed.binance.org
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Database (Optional - for historical data)
DATABASE_URL=postgresql://user:password@localhost:5432/arbitrage_bot
REDIS_URL=redis://localhost:6379

# Monitoring & Analytics
SENTRY_DSN=your_sentry_dsn_here
VERCEL_ANALYTICS_ID=your_vercel_analytics_id_here
MIXPANEL_TOKEN=your_mixpanel_token_here

# Performance Monitoring
NEW_RELIC_LICENSE_KEY=your_newrelic_key_here
DATADOG_API_KEY=your_datadog_key_here

# Notification Services (Optional)
DISCORD_WEBHOOK_URL=your_discord_webhook_url_here
SLACK_WEBHOOK_URL=your_slack_webhook_url_here
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# Email Notifications
SENDGRID_API_KEY=your_sendgrid_api_key_here
NOTIFICATION_EMAIL=your_email@domain.com

# Security
JWT_SECRET=your_jwt_secret_here
API_RATE_LIMIT=100
CORS_ORIGIN=https://yourdomain.com

# Feature Flags
ENABLE_REAL_TRADING=false
ENABLE_NOTIFICATIONS=true
ENABLE_HISTORICAL_DATA=true
ENABLE_ADVANCED_ANALYTICS=true

# Development Settings
NODE_ENV=development
LOG_LEVEL=info
DEBUG_MODE=false

# Vercel Specific
VERCEL_URL=your-app.vercel.app
VERCEL_ENV=production